{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "out",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/investor_pitch.html",
            "dest": "/public/investor_pitch.html"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}