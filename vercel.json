{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "logo-merged.png",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/investor_pitch.html",
            "dest": "/public/investor_pitch.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}